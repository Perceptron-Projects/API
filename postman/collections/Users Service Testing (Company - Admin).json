		{
			"name": "create-branch",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "60abf3af-dff5-4139-b80a-5194ea8e7dd4",
						"exec": [
							"// Test for a successful response",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Define the expected response for the branch",
							"const expectedBranch = {",
							"    \"branchImageUrl\": \"https://media.istockphoto.com/id/908578348/vector/business-building-illustration.jpg?s=612x612&w=0&k=20&c=thg6Bom79dCRo8pMV3fo7p-8b7m1p-EdLZZPKYpXYvg=\",",
							"    \"branchId\": \"65945d6b-20e4-4db9-a29d-052a8651453b\",",
							"    \"radiusFromCenterOfBranch\": \"50\",",
							"    \"branchName\": \"Test Branch\",",
							"    \"location\": {",
							"        \"longitude\": \"-122.4194\",",
							"        \"latitude\": \"37.7749\"",
							"    },",
							"    \"branchEmail\": \"testbranch@example.com\",",
							"    \"contactNo\": \"1234567890\"",
							"};",
							"",
							"// Test to check if the branch is added correctly",
							"pm.test(\"Branch is added correctly\", function () {",
							"    const responseBody = pm.response.json();",
							"    pm.expect(responseBody.branches).to.be.an(\"array\").that.includes.deep.members([expectedBranch]);",
							"});",
							"",
							"// Define the expected response for the company",
							"const expectedCompany = {",
							"    \"companyId\": pm.globals.get(\"testCompanyId\"),",
							"    \"companyImageUrl\": \"https://media.istockphoto.com/id/908578348/vector/business-building-illustration.jpg?s=612x612&w=0&k=20&c=thg6Bom79dCRo8pMV3fo7p-8b7m1p-EdLZZPKYpXYvg=\",",
							"    \"companyName\": \"testCompanyName\",",
							"    \"contactNo\": \"1234567890\",",
							"    \"companyEmail\": \"testCompany@gmail.com\"",
							"};",
							"",
							"// Test to check if the company is updated correctly",
							"pm.test(\"Company is updated correctly\", function () {",
							"    const responseBody = pm.response.json();",
							"    pm.expect(responseBody).to.include(expectedCompany);",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
