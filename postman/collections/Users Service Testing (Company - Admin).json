							"// Test for a successful response",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Test for userId",
							"pm.test(\"Response has userId\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"userId\").to.be.a('string');",
							"});",
							"",
							"// Test for companyId",
							"pm.test(\"Response has companyId\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"companyId\",pm.variables.get(\"testCompanyId\"));",
							"});",
							"",
							"// Test for contactNo",
							"pm.test(\"Response has contactNo\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"contactNo\", \"1234567890\");",
							"});",
							"",
							"// Test for role",
							"pm.test(\"Response has role\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"role\", \"branchadmin\");",
							"});",
							"",
							"// Test for email",
							"pm.test(\"Response has email\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"email\", pm.variables.get(\"testBranchAdminEmail\"));",
							"});",
							"",
							"",
							"",
							"// Test for firstName",
							"pm.test(\"Response has firstName\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"firstName\", \"John\");",
							"});",
							"",
							"// Test for lastName",
							"pm.test(\"Response has lastName\", function () {",
							"    pm.expect(pm.response.json()).to.have.property(\"lastName\", \"Doe\");",
							"});",
